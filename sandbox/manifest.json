{

	"manifest_version":	3,
    "name": "sandbox",
    "description": "sandbox",
    "version": "1",

    "action": {
        "default_popup": "popup.html",
        "default_title": "sandbox"
    },

    
    "content_scripts": [
        {
            "matches" : [
                "<all_urls>"
            ],
            "js": ["script.js"]
        }
	],
    
    "permissions": [
        "tabs",
        "activeTab"
    ],

    "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
    }
}
